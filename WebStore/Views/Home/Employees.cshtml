@model IEnumerable<WebStore.Models.Employee>

<h3>Сотрудники</h3>

<table>
    <thead>
    <tr>
        <th>Id</th>
        <th>Фамилия</th>
        <th>Имя</th>
        <th>Отчество</th>
        <th>Возраст</th>
    </tr>
    </thead>
    <tbody>
    @foreach (var employee in Model)
    {
        <tr>
            <td>@employee.Id</td>
            <td>
                <a href="/Home/EmployeDetails/@employee.Id">
                    @employee.LastName
                </a>
            </td>
            <td>@employee.FirstName</td>
            <td>@employee.Patronymic</td>
            <td>@employee.Age</td>
        </tr>
    }
    </tbody>
</table>

<a href="/">На главную</a>